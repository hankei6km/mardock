{
  "name": "mardock",
  "version": "0.1.0",
  "scripts": {
    "api:build": "aspida",
    "dirs:build": "sh scripts/dirs_build.sh",
    "licenses:build": "sh scripts/open_source_licenses.sh",
    "about:build": "sh scripts/about_license.sh",
    "copy:scripts": "sh scripts/copy_scripts.sh",
    "config:fetch": "sh scripts/fetch_config.sh",
    "pre": "yarn api:build && yarn dirs:build && yarn copy:scripts && yarn config:fetch",
    "dev": "yarn pre && next",
    "build": "yarn pre && next build && node scripts/purge_cache.js && find .mardock/cache -name '*.png'",
    "build:vercel": "yarn pre && yum install zip -y && yarn licenses:build && yarn build",
    "export": "next export",
    "start": "yarn pre && next start",
    "type-check": "tsc",
    "test": "jest"
  },
  "dependencies": {
    "@aspida/fetch": "^1.5.0",
    "@marp-team/marp-cli": "^0.23.3",
    "@marp-team/marp-core": "^1.4.3",
