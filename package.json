    "lite-youtube-embed": "^0.1.3",
    "mdast-qrcode": "0.2.0",
    "merge": "^2.1.1",
    "mix-css-color": "^0.1.1",
    "next": "10.0.8-canary.1",
    "next-transpile-modules": "^6.3.0",
    "notistack": "^1.0.3",
    "qrcode": "^1.4.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-material-ui-carousel": "2.2.4",
    "react-scroll": "^1.8.2",
    "rehype-minify-whitespace": "^4.0.5",
